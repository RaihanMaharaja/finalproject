<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="detailDivisiVM" type="com.raihansapplication.app.modules.detaildivisi.data.viewmodel.DetailDivisiVM"/>
    </data>
    <LinearLayout android:id="@+id/linearDetailDivisi"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylewhite_A700">
            <FrameLayout android:id="@+id/frameStackarrowleft"
                android:layout_height="@dimen/_56pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearBack"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.47"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_marginEnd="@dimen/_81pxh"
                            android:layout_marginStart="@dimen/_7pxh"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtRightButton"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_75pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{detailDivisiVM.detailDivisiModel.txtRightButton}"
                            style="@style/txtIntersemibold12" tools:text="@string/lbl_back"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPageTitle"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.53"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{detailDivisiVM.detailDivisiModel.txtPageTitle}"
                        style="@style/txtInterextrabold20" tools:text="@string/lbl_detail_divisi"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameNavBar"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_56pxv"
                    android:layout_width="@dimen/_374pxh" style="@style/groupStylewhite_A700">
                    <LinearLayout
                        android:id="@+id/linearRowarrowleftOne"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginEnd="@dimen/_23pxh"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearBack1"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.47"
                            android:layout_width="0dp" android:orientation="vertical">
                            <ImageView
                                android:id="@+id/imageArrowleftOne"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_marginEnd="@dimen/_81pxh"
                                android:layout_marginStart="@dimen/_7pxh"
                                android:layout_width="@dimen/_15pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_arrowleft"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtRightButtonOne"
                                android:layout_gravity="start"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_75pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{detailDivisiVM.detailDivisiModel.txtRightButtonOne}"
                                style="@style/txtIntersemibold12" tools:text="@string/lbl_back"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPageTitleOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_weight="0.53"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{detailDivisiVM.detailDivisiModel.txtPageTitleOne}"
                            style="@style/txtInterextrabold20" tools:text="@string/lbl_detail_divisi"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearNavBar"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylewhite_A700">
                        <LinearLayout
                            android:id="@+id/linearRowarrowleftTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_12pxv"
                            android:layout_marginStart="@dimen/_23pxh"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_weight="0.92"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:id="@+id/linearBack2"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.47"
                                android:layout_width="0dp" android:orientation="vertical">
                                <ImageView
                                    android:id="@+id/imageArrowleftTwo"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_12pxh"
                                    android:layout_marginEnd="@dimen/_81pxh"
                                    android:layout_marginStart="@dimen/_7pxh"
                                    android:layout_width="@dimen/_15pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_arrowleft"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtRightButtonTwo"
                                    android:layout_gravity="start"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_75pxh"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{detailDivisiVM.detailDivisiModel.txtRightButtonTwo}"
                                    style="@style/txtIntersemibold12" tools:text="@string/lbl_back"/>
                            </LinearLayout>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtPageTitleTwo"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginTop="@dimen/_3pxv"
                                android:layout_weight="0.53"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{detailDivisiVM.detailDivisiModel.txtPageTitleTwo}"
                                style="@style/txtInterextrabold20" tools:text="@string/lbl_detail_divisi"/>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageEdit"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_17pxv"
                            android:layout_marginEnd="@dimen/_19pxh"
                            android:layout_marginStart="@dimen/_89pxh"
                            android:layout_marginTop="@dimen/_19pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_edit"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_edit"/>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </androidx.appcompat.widget.Toolbar>
        <TextView android:ellipsize="end" android:id="@+id/txtTitle"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{detailDivisiVM.detailDivisiModel.txtTitle}"
            style="@style/txtInterregular14" tools:text="@string/lbl_nama_divisi"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtDescription"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.01"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{detailDivisiVM.detailDivisiModel.txtDescription}"
            style="@style/txtInterregular12" tools:text="@string/lbl_marketing"/>
        <View android:background="@color/bluegray_100"
            android:id="@+id/lineDivider"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_343pxh"/>
        <TextView android:ellipsize="end" android:id="@+id/txtTitleOne"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{detailDivisiVM.detailDivisiModel.txtTitleOne}"
            style="@style/txtInterregular14" tools:text="@string/msg_nama_kepala_div"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtDescriptionOne"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.01"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{detailDivisiVM.detailDivisiModel.txtDescriptionOne}"
            style="@style/txtInterregular12" tools:text="@string/lbl_brooke_davis"/>
        <View android:background="@color/bluegray_100"
            android:id="@+id/lineDividerOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_21pxv" android:layout_width="@dimen/_343pxh"/>
        <TextView android:ellipsize="end" android:id="@+id/txtTitleTwo"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{detailDivisiVM.detailDivisiModel.txtTitleTwo}"
            style="@style/txtInterregular14" tools:text="@string/lbl_no_telp"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtDescriptionTwo"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.01"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{detailDivisiVM.detailDivisiModel.txtDescriptionTwo}"
            style="@style/txtInterregular12" tools:text="@string/lbl_082317155099"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtTitleThree"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_39pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{detailDivisiVM.detailDivisiModel.txtTitleThree}"
            style="@style/txtInterregular14" tools:text="@string/lbl_email"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtDescriptionThree"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.01"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{detailDivisiVM.detailDivisiModel.txtDescriptionThree}"
            style="@style/txtInterregular12" tools:text="@string/msg_brooke_davis_sa"/>
        <View android:background="@color/bluegray_100"
            android:id="@+id/lineDividerTwo"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_343pxh"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearTabbar"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginTop="@dimen/_312pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearHomeButton"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_27pxv"
                android:layout_marginEnd="@dimen/_163pxh"
                android:layout_marginStart="@dimen/_163pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_500_47cornerRadius">
                <ImageView android:id="@+id/imageHome"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtTabName"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{detailDivisiVM.detailDivisiModel.txtTabName}"
                    style="@style/txtIntersemibold10" tools:text="@string/lbl_home"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
